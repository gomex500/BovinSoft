{
  "expo": {
    "name": "BovinSoft",
    "slug": "bovinsoft",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/img/splashLogo.png",
      "resizeMode": "contain",
      "backgroundColor": "#1B4725"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon_Bovinsoft.jpg",
        "backgroundColor": "#ffffff"
      },
      "icon": "./assets/img/logoNotificacion.png",
      "permissions": [
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_FINE_LOCATION"
      ],
      "package": "com.odiseoj676.bovinsoft"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "780e3c6b-7d6d-49fc-ba8a-73680ad2eb53"
      },
      "router": {
        "origin": false
      }
    },
    "owner": "odiseoj676",
    "plugins": [
      "expo-font"
    ]
  }
}
